	"null-character": 32,
	"overlays": {
		"scene": {
			"x": 0,
			"y": 0,
			"title": "Scene",
			"default": "city_map",
			"background":true,
			"options": {
				"city_map": {
					"x": 0,
					"y": 48,
					"w": 384,
					"h": 224,
					"info": {
						"x": 40,
						"y": 160,
						"height": 16,
						"text": "At the center of the problem is the\nhuge gang known as Mad Gear. Mad Gear\ncontrols all of the major criminal\nactivity in the city."
					}
				},
				"office1": {
					"x": 0,
					"y": 496,
					"w": 384,
					"h": 224,
					"info": {
						"x": 72,
						"y": 144,
						"height": 24,
						"text": "[red]Hee hee hee, Mr.Haggar. So pleased\nto make your acquaintance.\nI believe you know who I am."
					}
				},
				"office2": {
					"x": 0,
					"y": 720,
					"w": 384,
					"h": 224,
					"info": {
						"x": 72,
						"y": 144,
						"height": 24,
						"text": "[red]Don't hang up! We have a little\nbusiness proposition for you...\nYour daughter for your cooperation."
					}
				},
				"office3": {
					"x": 0,
					"y": 944,
					"w": 384,
					"h": 224,
					"info": {
						"x": 72,
						"y": 144,
						"height": 24,
						"text": "[red]Not so fast, Mike\nTurn on your TV."
					}
				},
				"tv_closeup": {
					"x": 0,
					"y": 1840,
					"w": 384,
					"h": 224,
					"info": {
						"x": 72,
						"y": 144,
						"height": 24,
						"text": "You son of a...\nWhat have you done to her?"
					}
				},
				"guy_and_cody": {
					"x": 0,
					"y": 272,
					"w": 384,
					"h": 224,
					"info": {
						"x": 72,
						"y": 144,
						"height": 24,
						"text": "[purple]The Mad Gears must pay!\nShe's my friend, too. Count me in."
					}
				},
				"profile_cody": {
					"x": 0,
					"y": 1168,
					"w": 384,
					"h": 224,
					"info": {
						"x": 48,
						"y": 24,
						"height": 16,
						"text": "[title]Profile of CODY[/title]\n             CODY b. 4-18-1967\n             He is a martialarts expert.\n             He is especially\n             good with knives.\n             His girl friend, Jessica,\n             was kidnapped by the\n             Mad Gear gang."
					}
				},
				"profile_guy": {
					"x": 0,
					"y": 1392,
					"w": 384,
					"h": 224,
					"info": {
						"x": 48,
						"y": 24,
						"height": 16,
						"text": "[title]Profile of Guy[/title]\n             GUY b. 8-12-1965\n             He has mastered the art\n             of Ninjitsu and attacks\n             with unequaled speed.\n             He often catches his\n             opponents off guard with\n             his special“Off-the-wall”\n             jump."
					}
				},
				"profile_haggar": {
					"x": 0,
					"y": 1616,
					"w": 384,
					"h": 224,
					"info": {
						"x": 48,
						"y": 24,
						"height": 16,
						"text": "[title]Profile of Haggar[/title]\n             HAGGAR b. 9-3-1943\n             He is a former champion\n             Street Fighter. He's the\n             new Mayor of Metro City.\n             He has mastered professional\n             wrestling skills and is\n             expert at the BACK DROP\n             and the PILE DRIVER."
					}
				},
				"blank": {
					"x": 0,
					"y": 2064,
					"w": 384,
					"h": 224,
					"info": {
						"x": 96,
						"y": 96,
						"height": 16,
						"text": "Sometime in the 1990's..."
					}
				},
				"haggar_cradling1": {
					"x": 0,
					"y": 2288,
					"w": 384,
					"h": 224,
					"info": {
						"x": 192,
						"y": 32,
						"height": 8,
						"text": "[smallpurple]Oh Father!\n\n    I was so scared...\n\n\n[smallblue]I'm so glad to see\n\nthey didn't hurt you.\n\nI'm so sorry, Jessica."
					}
				},
				"haggar_cradling2": {
					"x": 0,
					"y": 2512,
					"w": 384,
					"h": 224,
					"info": {
						"x": 192,
						"y": 32,
						"height": 8,
						"text": "[smallpurple]Oh Father!\n\n    I was so scared...\n\n\n[smallblue]I'm so glad to see\n\nthey didn't hurt you.\n\nI'm so sorry, Jessica.\n\nI thought I'd lost you\n\nlike I lost your mother.\n\nI'll never let anything\n\nbad happen to you again.\n\n\n[smallpurple]I love you Father."
					}
				},
				"kiss1": {
					"x": 0,
					"y": 2736,
					"w": 384,
					"h": 224,
					"info": {
						"x": 192,
						"y": 32,
						"height": 8,
						"text": "[smallpurple]\nWhere are you going?\n\nHow can you just walk\n\n            away now?\n\n[smallblue]\nI want to stay here \n\nwith you Jessica,\n\nbut I can't... not\n\nwhile evil still stalks\n\nthe streets.\n\n[smallpurple]\nOh Cody..."
					}
				},
				"kiss2": {
					"x": 0,
					"y": 2960,
					"w": 384,
					"h": 224,
					"info": {
						"x": 192,
						"y": 32,
						"height": 8,
						"text": "[smallpurple]\nWhere are you going?\n\nHow can you just walk\n\n            away now?\n\n[smallblue]\nI want to stay here \n\nwith you Jessica,\n\nbut I can't... not\n\nwhile evil still stalks\n\nthe streets.\n\n[smallpurple]\nOh Cody..."
					}
				},
				"kiss3": {
					"x": 0,
					"y": 3184,
					"w": 384,
					"h": 224,
					"info": {
						"x": 192,
						"y": 32,
						"height": 8,
						"text": "[smallpurple]\nWhere are you going?\n\nHow can you just walk\n\n            away now?\n\n[smallblue]\nI want to stay here \n\nwith you Jessica,\n\nbut I can't... not\n\nwhile evil still stalks\n\nthe streets.\n\n[smallpurple]\nOh Cody...\n\n\n            [title]End"
					}
				}
			}
		}
	},
