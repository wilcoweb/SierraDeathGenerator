	"hooks": {
		"pre-parse": "{fontInfo['height']=fontInfo['overlays']['scene']['options'][$('#overlay-scene option:selected').text()]['info']['height']}",
		"pre-text": "{let fnt = overlays.scene.data.info;mainFont.y = fnt['y-origin']; originx = fnt['x-origin'];fontInfo['wrap-width']=fnt.wrap}"
	},
	"notes":"Fonts available: [white] [yellow] [red] [blue] [big]",
